
SUBROUTINE momentum(pcorr,step) 
  !----------------------------------------------------                   
  USE header
!  use relaxation
  INTEGER step,i,j,k 
  REAL(kind=rc_kind) ::  dtim,fdiv,cfcdiv,ctrdiv,edt,pcorr(maxout),temp 
  REAL(kind=rc_kind) :: tarray(2),tim 
  REAL(kind=rc_kind) :: var(1:NI)
  integer ivb,ivs,ivf

  oldh= h

  do ivb=1,1

   if(ivb==1) then; dtim=dtf/3.d0; ivs=0;ivf=1;endif;
   if(ivb==2) then; dtim=0.5d0*dtf;ivs=1;ivf=1;endif;
   if(ivb==3) then; dtim=dtf      ;ivs=1;ivf=0;endif;

  CALL findzall 
  CALL sigma 

  var=u(1:NI,160,16,0);PRINT*,"S ",step," u ",MAXVAL(var)-MINVAL(var)
  var=v(1:NI,160,16,0);PRINT*,"S ",step," v ",MAXVAL(var)-MINVAL(var)
  var=w(1:NI,160,16,0);PRINT*,"S ",step," w ",MAXVAL(var)-MINVAL(var)
  var=s(1:NI,160,16,0);PRINT*,"S ",step," s ",MAXVAL(var)-MINVAL(var)
  var=p(1:NI,160,16  );PRINT*,"S ",step," p ",MAXVAL(var)-MINVAL(var)
 

  CALL advection_and_mixing(ivs,ivf,dtim,step)

  var=u(1:NI,160,16,1);PRINT*,step," u ",MAXVAL(var)-MINVAL(var)
  var=v(1:NI,160,16,1);PRINT*,step," v ",MAXVAL(var)-MINVAL(var)
  var=w(1:NI,160,16,1);PRINT*,step," w ",MAXVAL(var)-MINVAL(var)
  var=s(1:NI,160,16,1);PRINT*,step," s ",MAXVAL(var)-MINVAL(var)
  var=p(1:NI,160,16  );PRINT*,step," p ",MAXVAL(var)-MINVAL(var)
 
  CALL intpol 
 if(.NOT.(use_Shchepetkin)) then 
    CALL rpevalgrad_Song(ivf) 
   else
    CALL rpevalgrad_Song(ivf) 
    CALL rpevalgrad_Sche(ivf);
    drpx(1:NI,1:NJ,1:NK)=ru4_Sche(1:NI,1:NJ,1:NK);  drpy(1:NI,1:NJ,1:NK)=rv4_Sche(1:NI,1:NJ,1:NK); grpifc(0:NI,1:NJ,1:NK)=ru2_Sche(0:NI,1:NJ,1:NK); grpjfc(1:NI,0:NJ,1:NK)=rv2_Sche(1:NI,0:NJ,1:NK);
 endif

  var=drpx(1:NI,160,16);PRINT*,step," drpx ",MAXVAL(var)-MINVAL(var)
  var=drpy(1:NI,160,16);PRINT*,step," drpy ",MAXVAL(var)-MINVAL(var)


  CALL coriolis(ivs) 

  var=si(1:NI,160,16);PRINT*,step," si ",MAXVAL(var)-MINVAL(var)
  var=sj(1:NI,160,16);PRINT*,step," sj ",MAXVAL(var)-MINVAL(var)
  var=sk(1:NI,160,16);PRINT*,step," sk ",MAXVAL(var)-MINVAL(var)



  CALL srcface(ivs,step)      

  var=sifc(1:NI,160,16);PRINT*,step," sifc ",MAXVAL(var)-MINVAL(var)
  var=sjfc(1:NI,160,16);PRINT*,step," sjfc ",MAXVAL(var)-MINVAL(var)



  CALL hsolve(h,oldh,hdt,dtim)  
  var=h(1:NI,160);PRINT*,step," h ",MAXVAL(var)-MINVAL(var)
  CALL calcskfc                 
  var=skfc(1:NI,160,16);PRINT*,step," skfc ",MAXVAL(var)-MINVAL(var)
  CALL vhydro(dtim)             
  var=cxf(1:NI,160,16);PRINT*,step," cxf ",MAXVAL(var)-MINVAL(var)
  var=cyf(1:NI,160,16);PRINT*,step," cyf ",MAXVAL(var)-MINVAL(var)
  var=czf(1:NI,160,16);PRINT*,step," czf ",MAXVAL(var)-MINVAL(var)
  CALL cfdiv(cfcdiv)   
  CALL newcor(dtim,ivs)           
  CALL newsrc                   
  edt= EPS/dtim 
  CALL mgrid(pcorr,dtim,edt,cfcdiv)

  CALL vface(pcorr,dtim)           
  var=uf(1:NI,160,16);PRINT*,step," uf ",MAXVAL(var)-MINVAL(var)
  var=vf(1:NI,160,16);PRINT*,step," vf ",MAXVAL(var)-MINVAL(var)
  var=wf(1:NI,160,16);PRINT*,step," wf ",MAXVAL(var)-MINVAL(var)
  CALL vcenter(pcorr,dtim,ivf)       
  IF (fnhhy.NE.0) CALL pcorrect(pcorr)
  CALL facediv(dtim,fdiv)             
  CALL cdiv(dtim,ctrdiv,ivf)            

  var=u(1:NI,160,16,1);PRINT*,"E ",step," u ",MAXVAL(var)-MINVAL(var)
  var=v(1:NI,160,16,1);PRINT*,"E ",step," v ",MAXVAL(var)-MINVAL(var)
  var=w(1:NI,160,16,1);PRINT*,"E ",step," w ",MAXVAL(var)-MINVAL(var)
  var=s(1:NI,160,16,1);PRINT*,"E ",step," s ",MAXVAL(var)-MINVAL(var)
  var=p(1:NI,160,16  );PRINT*,"E ",step," p ",MAXVAL(var)-MINVAL(var)
  DO k=1,NK
    var=p(1:NI,160,k);PRINT*,k," p ",MAXVAL(var)-MINVAL(var)
  ENDDO

 

enddo

u(:,:,:,0)=u(:,:,:,1)
v(:,:,:,0)=v(:,:,:,1)
w(:,:,:,0)=w(:,:,:,1)
s(:,:,:,0)=s(:,:,:,1)

DO i=1,NI
  p(i,:,:)=p(1,:,:)
  h(i,:)=h(1,:)
  oldh(i,:)=oldh(1,:)
ENDDO


  CALL evalrho(rho,0) 
  CALL conadjust(step,0) 
  CALL diag_n2 
END SUBROUTINE momentum
